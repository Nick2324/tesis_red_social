package com.sna_deportivo.utils;

/**
 * 
 * Clase que contiene las constantes relacionadas a las relaciones en la
 * base de datos
 * 
 * @author Nicolas Mauricio Garcia Garzon
 * @version 1.0
 *
 */
public class Relaciones {
	//************************Eventos deportivos**************************
	/**
	 * 
	 * Relación entre un {@link com.sna_deportivo.utils.Entidades.#EVENTO} y su 
	 * {@link com.sna_deportivo.utils.Entidades.#USUARIO} creador
	 * 
	 */
	public static final String CREAEVENTO = "";	
	/**
	 * 
	 * Mapea la relación n-aria entre {@link com.sna_deportivo.utils.Entidades.#DEPORTEEVENTO} y las siguientes:
	 * 
	 * - {@link com.sna_deportivo.utils.Entidades.#DEPORTE}
	 * - {@link com.sna_deportivo.utils.Entidades.#GENERO}
	 * - {@link com.sna_deportivo.utils.Entidades.#DEPORTE}
	 * - {@link com.sna_deportivo.utils.Entidades.#EVENTODEPORTIVO}
	 * - {@link com.sna_deportivo.utils.Entidades.#UBICACION}
	 * 
	 */
	public static final String EVENTODEDEPORTE = "";
	/**
	 * 
	 * Relación entre {@link Entidades.#USUARIO} y {@link Entidades.#DEPORTEEVENTO}
	 * 
	 */
	public static final String PARTICIPANTEEVENTO = "";
	
	
	private Relaciones(){}
	
}
